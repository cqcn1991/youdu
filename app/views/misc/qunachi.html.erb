<div class="qunachi-container">
  <div class="map">
    <%= render('map') %>
  </div>

  <div class="restaurants">
    <div class="multiple-items" data-slick='{"slidesToShow": 3, "slidesToScroll": 3}'>
      <% 4.times do %>
      <div class="restaurant">
        <img class="img-responsive" src="http://p1.meituan.net/merchantpic/311b9072de07363ed02ce06540826abb3337196.jpg%40280w_212h_1e_1c_1l%7Cwatermark%3D0" alt="">
        <h4>￥618 上海 - 整套房子/公寓</h4>
        <div class="restaurant-rating">
          <% 5.times do  %>
              <span class="star"><svg viewBox="0 0 1000 1000" aria-hidden="true" focusable="false" style="display: block; fill: currentcolor; height: 1em; width: 1em;"><path d="M971.5 379.5c9 28 2 50-20 67L725.4 618.6l87 280.1c11 39-18 75-54 75-12 0-23-4-33-12l-226.1-172-226.1 172.1c-25 17-59 12-78-12-12-16-15-33-8-51l86-278.1L46.1 446.5c-21-17-28-39-19-67 8-24 29-40 52-40h280.1l87-278.1c7-23 28-39 52-39 25 0 47 17 54 41l87 276.1h280.1c23.2 0 44.2 16 52.2 40z"></path></svg></span>
          <% end  %>
          <span>175条评价</span>
        </div>
      </div>
      <% end  %>
    </div>
  </div>
</div>